:rockpaperscissors
cls
echo Welcome to Rock, Paper, Scissors!
echo.
echo Choose your move:
echo 1. Rock
echo 2. Paper
echo 3. Scissors
echo.

set /p "user_choice=Enter the number of your choice (1-3): "

if "%user_choice%"=="1" (
    set user_move=Rock
) else if "%user_choice%"=="2" (
    set user_move=Paper
) else if "%user_choice%"=="3" (
    set user_move=Scissors
) else (
    echo Invalid choice! Please enter a number between 1 and 3.
    pause
    goto rockpaperscissors
)

set /a computer_choice=%random% %% 3 + 1

if %computer_choice%==1 (
    set computer_move=Rock
) else if %computer_choice%==2 (
    set computer_move=Paper
) else if %computer_choice%==3 (
    set computer_move=Scissors
)

echo.
echo You chose: %user_move%
echo Computer chose: %computer_move%
echo.

if "%user_move%"=="%computer_move%" (
    echo It's a tie!
) else if "%user_move%"=="Rock" (
    if "%computer_move%"=="Scissors" (
        echo You win!
    ) else (
        echo You lose!
    )
) else if "%user_move%"=="Paper" (
    if "%computer_move%"=="Rock" (
        echo You win!
    ) else (
        echo You lose!
    )
) else if "%user_move%"=="Scissors" (
    if "%computer_move%"=="Paper" (
        echo You win!
    ) else (
        echo You lose!
    )
)

echo.
echo Do you want to play again? (y/n)
set /p "play_again=Enter y to play again, n to exit: "
if /i "%play_again%"=="y" (
    goto rockpaperscissors
) else (
    echo Thanks for playing! Goodbye!
    pause
    goto start

echo.

set /a target=%random% %% 10 + 1
set attempts=0

:guess
set /p "user_guess=Enter your guess (1-10): "
set /a attempts+=1

if "%user_guess%"=="%target%" (
    echo Congratulations! You guessed the right number in %attempts% attempts!
    goto endgame
) else (
    if %user_guess% LSS %target% (
        echo Your guess is too low. going back
    ) else (
        echo Your guess is too high. going back
    )
    goto endgame
)

